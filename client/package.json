{
  "name": "cloud-inspector",
  "version": "0.0.8",
  "description": "Tool to manage cloud deployments",
  "repository": {
    "type": "git"
  },
  "license": "MIT",
  "scripts": {
    "start": "webpack serve --config config/webpack.config.dev.ts",
    "build": "rimraf dist/ && npm run build:server && webpack --progress --config config/webpack.config.prod.ts",
    "build:server": "tsc --project server/tsconfig.json",
    "server": "node dist/server.js",
    "clean": "rimraf dist/",
    "lint": "eslint --ext .js --ext .jsx --ext .ts --ext .tsx src/",
    "stylelint": "stylelint './src/**/*.ts*'",
    "analyzer": "webpack --config config/webpack.config.prod.ts --profile --json > stats.json && webpack-bundle-analyzer stats.json ./dist",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "react",
    "webpack",
    "express",
    "redux",
    "boilerplate",
    "babel",
    "styled-components",
    "typescript"
  ],
  "browserslist": [
    "> 0.2%",
    "ie 11",
    "not dead",
    "not op_mini all"
  ],
